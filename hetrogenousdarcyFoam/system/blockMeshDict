FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

convertToMeters 1;
Lx 10;
Ly 6;
Lz 0.1;


vertices
(
    (0 0 0)
    ($Lx 0 0)
    ($Lx  $Ly  0)
    (0 $Ly 0)
    (0 0 $Lz)
    ($Lx 0 $Lz)
    ($Lx  $Ly $Lz)
    (0 $Ly $Lz)
);

blocks
(
    hex (0 1 2 3 4 5 6 7) (60 60 1) simpleGrading (1 1 1)
);

edges
(
);

boundary (

inlet {

            type patch;
		faces (
                   (0 4 7 3)  

                 );

		}


outlet {

             type patch;
		faces (
                   (2 6 5 1)  

                 );

		}


frontAndback {

            type empty;
		faces (
                   (1 5 4 0)  
                   (3 7 6 2) 
                   (0 3 2 1) 
                   (4 5 6 7) 
		   );

		}

	
		
		
		
);





