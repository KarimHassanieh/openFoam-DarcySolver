FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //



defaultFieldValues
(
    volScalarFieldValue k 1e-9
);

regions
(
    // Set patch values (using ==)
    boxToFace
    {
        box (0 0 0) ( 5 3 0);

        fieldValues
        (
            volScalarFieldValue k 5e-9
        );
    }
);


